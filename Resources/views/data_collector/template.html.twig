{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block menu %}
    <span class="label">
        <span class="icon"><img src="data:image/png;base64," alt="Twig constants" width="32" height="32" /></span>
        <strong>Twig Constants</strong>
    </span>
{% endblock %}

{% block panel %}
    <h2>Constants</h2>
    <table>
        {% for class, data in collector.constants %}
            <tr>
                <th colspan="2">{{ class }}</th>
            </tr>

            {% for key, value in data %}
                <tr>
                    <th>{{ key }}</th>
                    <td>{{ value }}</td>
                </tr>
            {% endfor %}
        {% else %}
            <tr>
                <td colspan="2" style="text-align: center;">No constants declared.</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}
